   241              WS-PGMID = 'SM002' OR WS-PGMID = 'SM003' OR
   242              WS-PGMID = 'SM004' OR WS-PGMID = 'SM005' OR
   243              WS-PGMID = 'SM006'
   244		     IF WS-STATE NOT = LOW-VALUES
   245			PERFORM 200-REC-MAP
   246			PERFORM 500-CHECK-EIBAID
   247			EVALUATE WS-STATE
   248			  WHEN 1
   249			       PERFORM 600-MOVE-Q-TO-SCREEN
   250			       PERFORM 111-CREATE-MAP
   251			  WHEN 2
   252			       PERFORM 111-CREATE-MAP
   253			END-EVALUATE
   254		     ELSE
   255			 MOVE WS-SELECT-OPTION TO ERRMSG1O
   256			 MOVE 1 TO WS-STATE
   257			 MOVE 1 TO WS-PAGE
   258			 MOVE WS-PAGE TO PAGEO
   259			 MOVE -1 TO TITLEL
   260			 PERFORM 900-MOVE-FILES-TO-Q
   261			 PERFORM 600-MOVE-Q-TO-SCREEN
   262			 PERFORM 111-CREATE-MAP
   263		     END-IF
   264		 ELSE
   265		     MOVE SPACES TO ERRMSG1O
   266		     MOVE LENGTH OF WS-INVALID-ACCESS TO WS-LENGTH
   267		     EXEC CICS SEND TEXT
   268			     FROM (WS-INVALID-ACCESS)
   269			     LENGTH (WS-LENGTH)
   270			     ERASE
   271			END-EXEC
   272		     EXEC CICS RETURN
   273		     END-EXEC
   274		 END-IF.
