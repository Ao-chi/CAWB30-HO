   246           IF WS-PGMID = 'SM000' OR WS-PGMID = 'SM001' OR
   247              WS-PGMID = 'SM002' OR WS-PGMID = 'SM003' OR
   248              WS-PGMID = 'SM004' OR WS-PGMID = 'SM005' OR
   249              WS-PGMID = 'SM006'
   250		     IF WS-STATE NOT = LOW-VALUES
   251			PERFORM 200-REC-MAP
   252			PERFORM 500-CHECK-EIBAID
   253			PERFORM 600-MOVE-Q-TO-SCREEN
   254			PERFORM 111-CREATE-MAP
   255
